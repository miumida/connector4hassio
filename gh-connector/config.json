{
  "name": "GH-Connector",
  "version": "1.0.0",
  "slug": "ghconnector",
  "description": "GH-Connector for Hass.io",
  "uart": true,
  "url": "https://github.com/miumida/connector4hassio",
  "host_network":true,
  "startup": "services",
  "arch": [
    "amd64",
    "armv7"
  ],
  "boot": "auto",
  "ingress": true,
  "ingress_port": 30010,
  "map": [
    "share:rw",
    "config:rw"
  ],
  "panel_icon": "mdi:alpha-g-box",
  "webui": "http://[HOST]:[PORT:30010]",
  "ports": {
    "30010/tcp": 30010
  },
  "ports_description": {
    "30010/tcp": "Web interface (Not required for Hass.io Ingress)"
  },
  "options": {
    "data_path": "/share/ghconnector",
    "homeassistant": true
  },
  "schema": {
    "data_path": "str",
    "homeassistant": "bool"
  }
}
