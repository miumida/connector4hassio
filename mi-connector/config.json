{
  "name": "Mi-Connector",
  "version": "1.0.0",
  "slug": "miconnector",
  "description": "Mi-Connector for Hass.io",
  "uart": true,
  "url": "https://github.com/miumida/connector4hassio",
  "host_network":true,
  "startup": "services",
  "arch": [
    "amd64",
    "armv7"
  ],
  "boot": "auto",
  "map": [
    "share:rw",
    "config:rw"
  ],
  "panel_icon": "mdi:alpha-m-box",
  "webui": "http://[HOST]:[PORT:30000]",
  "ports": {
    "30000/tcp": 30000
  },
  "ports_description": {
    "30000/tcp": "Web interface (Not required for Hass.io Ingress)"
  },
  "options": {
    "data_path": "/share/lg-connector",
    "homeassistant": true
  },
  "schema": {
    "data_path": "str",
    "homeassistant": "bool"
  }
}
